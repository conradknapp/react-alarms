{
  "name": "react-alarms",
  "version": "1.0.0",
  "main": "server.js",
  "author": "Conrad Knapp",
  "license": "MIT",
  "engines": {
    "node": "8.11.1",
    "npm": "5.6.0",
    "yarn": "1.6.0"
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && yarn start",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "heroku-postbuild":
      "NPM_CONFIG_PRODUCTION=false yarn install --prefix client && yarn run build --prefix client"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "mongoose": "^5.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.17.4"
  }
}
